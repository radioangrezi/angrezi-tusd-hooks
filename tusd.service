[Unit]
Description=tus protocol file server 

[Service]
Type=simple
ExecStart=/usr/local/bin/tusd -upload-dir="." -behind-proxy -hooks-http "http://localhost:3002" -port 1080
WorkingDirectory=/media/storage/share/upload/
User=root

TimeoutStopSec=10
Restart=on-failure
RestartSec=5

User=angrezi
Group=angrezi

[Install]
WantedBy=default.target